@model IEnumerable<TicketsLC.Models.Ticket>

<div>
    <h4>Ticket</h4>
    
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.Id)</th>
                <th>@Html.DisplayNameFor(model => model.UsuarioId)</th>
                <th>@Html.DisplayNameFor(model => model.CategoriaId)</th>
                <th>@Html.DisplayNameFor(model => model.Titulo)</th>
                <th>@Html.DisplayNameFor(model => model.Menssage)</th>
                <th>@Html.DisplayNameFor(model => model.DestinatarioId)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Id)</td>           
                
                    <td>@Html.DisplayFor(modelItem => item.UsuarioId)</td>                
                
                    <td>@Html.DisplayFor(modelItem => item.CategoriaId)</td>
                
               
                    <td>@Html.DisplayFor(modelItem => item.Titulo)</td>
               
               
                    <td>@Html.DisplayFor(modelItem => item.Menssage)</td>
               
                
                    <td>@Html.DisplayFor(modelItem => item.DestinatarioId)</td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
